<Window x:Class="Awiiasails.result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Awiiasails"
        mc:Ignorable="d"
        Title="Данные о пассажирах" Height="513" Width="800">
    <Grid Margin="0,0,0,9">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="80*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="LB_result" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,121"/>
        <Label x:Name="label_result" Content="ИТОГО:" HorizontalAlignment="Left" Margin="25,390,0,0" VerticalAlignment="Top" Height="54" Width="571" FontSize="36" FontWeight="Bold"/>

    </Grid>
</Window>
